{
  "cli": {
    "version": ">= 9.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_SERVER_URL": "https://lavial-backend-production.up.railway.app/api"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_SERVER_URL": "https://lavial-backend-production.up.railway.app/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "aab",
        "image": "latest",
        "gradleCommand": ":app:bundleRelease"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_SERVER_URL": "https://lavial-backend-production.up.railway.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}