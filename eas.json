{
  "cli": {
    "version": ">= 9.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "gradleCommand": ":app:bundleRelease",
        "buildToolsVersion": "35.0.0"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_SERVER_URL": "https://lavial-backend-production.up.railway.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}