{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@env": ["@types/env.d.ts"]
    },
    "esModuleInterop": true
  },
  "include": [
    "./*.ts",
    "./*.tsx",
    "./pages/**/*.ts",
    "./pages/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "./node_modules/expo/tsconfig.base"
}
