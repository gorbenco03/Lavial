{
  "expo": {
    "name": "Lavial",
    "slug": "Lavial",
    "version": "2.0.0",
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/ClashGrotesk-Bold.otf",
            "./assets/fonts/ClashGrotesk-Extralight.otf",
            "./assets/fonts/ClashGrotesk-Light.otf",
            "./assets/fonts/ClashGrotesk-Medium.otf",
            "./assets/fonts/ClashGrotesk-Regular.otf",
            "./assets/fonts/ClashGrotesk-Semibold.otf"
          ]
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "lavialapp",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourapp.bundle",
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff",
        "versionCode" : 2
      },
      "icon": "./assets/icon.png",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.yourapp.bundle"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "381b83f1-efcf-4ec4-b0df-e45c8a2ca04f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/381b83f1-efcf-4ec4-b0df-e45c8a2ca04f"
    }
  }
}
