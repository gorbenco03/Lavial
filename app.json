{
  "expo": {
    "name": "Lavial",
    "slug": "Lavial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "lavialapp",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourapp.bundle",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "App needs access to photo library in order to save PDF",
        "NSPhotoLibraryAddUsageDescription": "App needs access to photo library in order to save PDF",
        "NSFileProviderDomainUsageDescription": "App needs access to files for PDF saving"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_round.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/icon.png",
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.yourapp.bundle"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "381b83f1-efcf-4ec4-b0df-e45c8a2ca04f"
      }
    }
  }
}
